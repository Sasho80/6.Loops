using System;

class NumbersFrom1ToN
{
    static void Main()
    {
        Console.WriteLine("Enter a positive integer:");
        int n = int.Parse(Console.ReadLine());
        if (n<=0)
        {
            Console.WriteLine("Invalid input!");
            return;
        }
        for (int i = 1; i <=n; i++)
        {
            Console.Write(i+" ");
        }
        Console.WriteLine();
    }
}

