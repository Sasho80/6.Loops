using System;

class MatrixOfNumbers
{
    static void Main()
    {
        Console.WriteLine("Enter n(1<=n<=20):");
        int n = int.Parse(Console.ReadLine());
        if (n < 1 || n > 20)
        {
            Console.WriteLine("Invalid input!");
            return;
        }
        for (int row = 1; row <=n; row++)
        {
            for (int i=1,col = row; i <=n; i++,col++)
            {
                Console.Write(col+" ");
            }
            Console.WriteLine();
        }
    }
}

