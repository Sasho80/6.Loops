using System;
using System.Numerics;

class CalculateTheQuotientOfFactorials
{
    static void Main()
    {
        Console.WriteLine("Enter n and k(1<k<n<100):");
        int n = int.Parse(Console.ReadLine());
        int k = int.Parse(Console.ReadLine());
        if (k<1||k>100||k>n||n>100||n<1)
        {
            Console.WriteLine("Invalid input!");
            return;
        }
        BigInteger nFactorial = 1;
        BigInteger kFactorial = 1;
        BigInteger nMinusKFactorial = 1;
        BigInteger quotient = 0;
        for (int i = 1,j=1,l=1; i <=n; i++,j++,l++)
        {
            nFactorial *= i;
            if (j<=k)
            {
                kFactorial *= j; 
            }
            if (l<=n-k)
            {
                nMinusKFactorial *= l; 
            }
        }
        quotient=nFactorial/(kFactorial*(nMinusKFactorial));
        Console.WriteLine(quotient);
    }
}

