using System;

class OddAndEvenProduct
{
    static void Main()
    {
        Console.WriteLine("Enter n integers separated by a space:");
        string[] str = Console.ReadLine().Split();
        int productOdd = 1;
        int productEven = 1;
        for (int i = 0; i < str.Length; i++)
        {
            if (i%2==0)
            {
                int elementOdd = Convert.ToInt32(str[i]);
                productOdd *= elementOdd;
            }
            else
            {
                int elementEven = Convert.ToInt32(str[i]);
                productEven *= elementEven;
            }
        }
        if (productEven==productOdd)
        {
            Console.WriteLine("Yes");
            Console.WriteLine("product={0}",productOdd);
        }
        else
        {
            Console.WriteLine("No");
            Console.WriteLine("odd_product={0}",productOdd);
            Console.WriteLine("even_product={0}",productEven);
        }
    }
}

