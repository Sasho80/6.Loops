using System;

class RandomizeTheNumbers1ToN
{
    static void Main()
    {
        Console.WriteLine("Enter integer n:");
        int n = int.Parse(Console.ReadLine());
        int[] RandomNum = new int[n];
        int random = 0;
        int checkRandom = 0; ;
        if (n<1)
        {
            Console.WriteLine("Invalid input!");
            return;
        }
        Random rnd = new Random();
        random = rnd.Next(1, n + 1);
        checkRandom = random;
        RandomNum[0] = random;
        for (int i = 1; i < n; i++)
        {
            random=rnd.Next(1, n + 1);
            if (checkRandom!=random)
            {
                RandomNum[i] = random;
                for (int j = 0; j < n; j++)
                {
                    if (j==i)
                    {
                        continue;
                    }
                    if (RandomNum[j] == RandomNum[i])
                    {
                        i--;
                        break;
                    }
                }
            }
            else
            {
                i--;
            }
        }
        Console.WriteLine();
        foreach (var item in RandomNum)
        {
            Console.Write(item+" ");
        }
        Console.WriteLine();
    }
}

