using System;
using System.Numerics;

class CatalanNumbers
{
    static void Main()
    {
        Console.WriteLine("Enter n (1<n<100):");
        int n = int.Parse(Console.ReadLine());
        if (n>100||n<0)
        {
            Console.WriteLine("Invalid input!");
            return;
        }
        BigInteger twoNFactorial=1;
        BigInteger nFactorial=1;
        BigInteger nPlusOneFactorial = 1;
        BigInteger CatalanNum = 0;
        for (int i = 1,j=1,k=1; i <=2*n; i++,j++,k++)
        {
            twoNFactorial *= i;
            if (j<=n)
            {
              nFactorial *= j;  
            }
            if (k<=n+1)
            {
                nPlusOneFactorial *= k; 
            }
        }
        CatalanNum = twoNFactorial/((nPlusOneFactorial) * nFactorial);
        Console.WriteLine(CatalanNum);
    }
}

